}

/* ====== Labels ====== */
QLabel {
    font-size: 20pt;
    font-weight: bold;
    color: #FFFFFF;
}

/* ====== Botões principais (QPushButton) ====== */
QPushButton {
    background-color: #1e3a5f;
    border-radius: 12px;
    padding: 14px;
    font-size: 15pt;
    font-weight: 500;
    color: #FFFFFF;
    border: none;
}
QPushButton:hover {
    background-color: #284c75;
}
QPushButton:pressed {
    background-color: #162a44;
}

/* ====== Botões de navegação (QToolButton) ====== */
QToolButton {
    background-color: #1e3a5f;
    border-radius: 12px;
    padding: 10px;
    font-size: 13pt;
    color: #FFFFFF;
    text-align: left;
    border: none;
}
QToolButton:hover {
    background-color: #284c75;
}
QToolButton:checked {
    background-color: #3b82f6;
    color: white;
}
QToolButton:checked:hover {
    background-color: #2563eb;
}

/* ====== Campos de texto ====== */
QLineEdit {
    background-color: #1e293b;
    border: 2px solid #334155;
    border-radius: 8px;
    padding: 6px 10px;
    font-size: 13pt;
    color: white;
}
QLineEdit:focus {
    border: 2px solid #3b82f6;
}

/* ====== Barra de rolagem ====== */
QScrollBar:vertical {
    background: transparent;
    width: 8px;
}
QScrollBar::handle:vertical {
    background: #64748b;
    border-radius: 4px;
}
QScrollBar::handle:vertical:hover {
    background: #94a3b8;
}
